{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-fileinfo": "*",
        "alperenersoy/filament-export": "^3.0",
        "aphisitworachorch/kacher": "^1.3",
        "ariaieboy/filament-jalali-datetimepicker": "^2.1",
        "bezhansalleh/filament-shield": "^3.1",
        "codewithdennis/filament-select-tree": "^3.1",
        "filament/filament": "3.*",
        "filament/notifications": "^3.3",
        "flowframe/laravel-trend": "^0.4.0",
        "guzzlehttp/guzzle": "^7.2",
        "hugomyb/filament-media-action": "v3.1.1.9",
        "ibrahim-bougaoua/filaprogress": "^0.1.0",
        "jaocero/filachat": "^0.2.0@alpha",
        "jeffgreco13/filament-breezy": "^2.6",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "maatwebsite/excel": "^3.1",
        "mokhosh/filament-jalali": "^4.6",
        "phpsa/filament-password-reveal": "^2.0",
        "pxlrbt/filament-excel": "^2.1",
        "rmsramos/activitylog": "^2.0",
        "saade/filament-fullcalendar": "3.0",
        "tomatophp/filament-pwa": "^1.0",
        "ext-libxml": "*",
        "ext-dom": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel-lang/common": "^4.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                ""
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
